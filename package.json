{
  "name": "detherjs",
  "version": "1.0.0",
  "description": "Dether SDK to easily call smart contract from Javascript",
  "main": "lib/index.js",
  "scripts": {
    "babel-node": "babel-node --presets=latest",
    "example": "npm run babel-node -- examples/usage.js",
    "test": "nyc --reporter=html mocha --compilers js:babel-core/register test/test.config.js test/specs/*.test.js test/specs/**/*.test.js --timeout 15000",
    "prepublish": "babel src --out-dir lib",
    "build": "babel src --out-dir lib",
    "esdoc": "./node_modules/.bin/esdoc",
    "publish:esdoc": "gh-pages -d esdoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dethertech/dether.js.git"
  },
  "keywords": [
    "Dether",
    "ethereum",
    "sdk"
  ],
  "author": "Dether Team <hello@dether.io>",
  "contributors": [
    "Mehdi Amari <mehdi@dether.io> (https://github.com/Fukunaga42)",
    "Pacien Boisson <pakokrew@gmail.com> (https://github.com/pakokrew)"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dethertech/dether.js/issues"
  },
  "homepage": "https://github.com/dethertech/dether.js#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "dethercontract": "^1.2.5",
    "ethers": "^2.1.2",
    "gh-pages": "^1.0.0",
    "utf8": "^2.1.2",
    "web3": "^1.0.0-beta.24"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-preset-latest": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "esdoc": "^1.0.2",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.7.2",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^3.5.3",
    "nyc": "^11.3.0",
    "sinon": "^4.0.2"
  }
}
